{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "12.8.3",
    "axios": "0.24.0",
    "cors": "^2.8.5",
    "formik": "2.2.9",
    "nodemon": "^2.0.15",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "^5.0.0",
    "web-vitals": "1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "fix-lint": "eslint --fix .",
    "service": "cd service && npm run dev",
    "dev": "concurrently \"npm start\" \"cd service/ && npm run dev\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "6.5.1",
    "eslint-config-prettier": "8.3.0",
    "prettier": "2.4.1"
  }
}
